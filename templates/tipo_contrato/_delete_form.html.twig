<form method="post" action="{{ path('tipo_contrato_delete', {'id': tipo_contrato.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ tipo_contrato.id) }}">
    <button class="btn">Delete</button>
</form>
